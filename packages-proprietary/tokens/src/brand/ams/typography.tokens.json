{
  "ams": {
    "typography": {
      "font-family": {
        "$value": "'Amsterdam Sans', Arial, sans-serif",
        "$type": "fontFamily"
      },
      "hyphenate-limit-chars": {
        "$value": "12 8 4"
      },
      "body-text": {
        "font-size": {
          "$value": "clamp(1.125rem, 1.0893rem + 0.1786vw, 1.25rem)",
          "$extensions": {
            "amsterdam.designsystem.type": "fontSize"
          }
        },
        "font-weight": {
          "$value": "400",
          "$type": "fontWeight"
        },
        "line-height": {
          "$value": "1.8",
          "$extensions": {
            "amsterdam.designsystem.type": "lineHeight"
          }
        },
        "bold": {
          "font-weight": {
            "$value": "800",
            "$type": "fontWeight"
          }
        },
        "small": {
          "font-size": {
            "$value": {
              "value": 1,
              "unit": "rem"
            },
            "$type": "dimension"
          },
          "line-height": {
            "$value": "1.6",
            "$extensions": {
              "amsterdam.designsystem.type": "lineHeight"
            }
          }
        },
        "large": {
          "font-size": {
            "$value": "clamp(1.3125rem, 1.2411rem + 0.3571vw, 1.5625rem)",
            "$extensions": {
              "amsterdam.designsystem.type": "fontSize"
            }
          },
          "line-height": {
            "$value": "1.6",
            "$extensions": {
              "amsterdam.designsystem.type": "lineHeight"
            }
          }
        },
        "x-large": {
          "font-size": {
            "$value": "clamp(1.5rem, 1.3571rem + 0.7143vw, 2rem)",
            "$extensions": {
              "amsterdam.designsystem.type": "fontSize"
            }
          },
          "line-height": {
            "$value": "1.4",
            "$extensions": {
              "amsterdam.designsystem.type": "lineHeight"
            }
          }
        }
      },
      "heading": {
        "font-weight": {
          "$value": "800",
          "$type": "fontWeight"
        },
        "text-wrap": {
          "$value": "balance"
        },
        "1": {
          "font-size": {
            "$value": "clamp(2rem, 1.7143rem + 1.4286vw, 3rem)",
            "$extensions": {
              "amsterdam.designsystem.type": "fontSize"
            }
          },
          "line-height": {
            "$value": "1.2",
            "$extensions": {
              "amsterdam.designsystem.type": "lineHeight"
            }
          }
        },
        "2": {
          "font-size": {
            "$value": "{ams.typography.body-text.x-large.font-size}",
            "$extensions": {
              "amsterdam.designsystem.type": "fontSize"
            }
          },
          "line-height": {
            "$value": "1.3",
            "$extensions": {
              "amsterdam.designsystem.type": "lineHeight"
            }
          }
        },
        "3": {
          "font-size": {
            "$value": "{ams.typography.body-text.large.font-size}",
            "$extensions": {
              "amsterdam.designsystem.type": "fontSize"
            }
          },
          "line-height": {
            "$value": "1.3",
            "$extensions": {
              "amsterdam.designsystem.type": "lineHeight"
            }
          }
        },
        "4": {
          "font-size": {
            "$value": "{ams.typography.body-text.font-size}",
            "$extensions": {
              "amsterdam.designsystem.type": "fontSize"
            }
          },
          "line-height": {
            "$value": "1.4",
            "$extensions": {
              "amsterdam.designsystem.type": "lineHeight"
            }
          }
        },
        "5": {
          "font-size": {
            "$value": "{ams.typography.body-text.small.font-size}",
            "$extensions": {
              "amsterdam.designsystem.type": "fontSize"
            }
          },
          "line-height": {
            "$value": "1.4",
            "$extensions": {
              "amsterdam.designsystem.type": "lineHeight"
            }
          }
        }
      }
    }
  }
}
