{
  "ams": {
    "progress-list": {
      "body": {
        "padding-block-start": { "value": "{ams.space.s}" },
        "padding-block-end": { "value": "{ams.space.l}" }
      },
      "completed": {
        "marker": {
          "background-color": { "value": "{ams.color.feedback.success}" }
        },
        "connector": {
          "background-color": { "value": "{ams.color.feedback.success}" }
        }
      },
      "connector": {
        "background-color": { "value": "{ams.color.separator}" },
        "inline-size": { "value": "{ams.border.width.m}" }
      },
      "current": {
        "marker": {
          "background-color": { "value": "{ams.color.feedback.success}" },
          "border-color": { "value": "{ams.color.background}" },
          "border-style": { "value": "solid" },
          "border-width": { "value": "0.2em" },
          "box-shadow": {
            "value": " 0 0 0 var(--ams-progress-list-current-marker-border-width) {ams.color.feedback.success}"
          }
        },
        "connector": {
          "background-color": { "value": "{ams.color.feedback.success}" }
        }
      },
      "indicator": {
        "padding-inline": { "value": "{ams.space.m}" }
      },
      "marker": {
        "background-color": { "value": "{ams.color.separator}" },
        "heading-2": {
          "font-size": { "value": "{ams.typography.heading.2.font-size}" },
          "line-height": { "value": "{ams.typography.heading.2.line-height}" }
        },
        "heading-3": {
          "font-size": { "value": "{ams.typography.heading.3.font-size}" },
          "line-height": { "value": "{ams.typography.heading.3.line-height}" }
        },
        "heading-4": {
          "font-size": { "value": "{ams.typography.heading.4.font-size}" },
          "line-height": { "value": "{ams.typography.heading.4.line-height}" }
        }
      },
      "medium": {
        "indicator": {
          "padding-inline": { "value": "{ams.space.l}" }
        }
      },
      "shape": {
        "heading-2": {
          "inline-size": { "value": "calc(var(--ams-progress-list-marker-heading-2-font-size) * 0.8)" }
        },
        "heading-3": {
          "inline-size": { "value": "calc(var(--ams-progress-list-marker-heading-3-font-size) * 0.8)" }
        },
        "heading-4": {
          "inline-size": { "value": "calc(var(--ams-progress-list-marker-heading-4-font-size) * 0.8)" }
        }
      },
      "upcoming": {
        "shape": {
          "background-color": { "value": "{ams.color.separator}" }
        }
      }
    }
  }
}
