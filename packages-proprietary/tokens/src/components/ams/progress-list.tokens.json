{
  "ams": {
    "progress-list": {
      "step": {
        "body": {
          "padding-block-end": { "value": "{ams.space.l}" },
          "padding-block-start": { "value": "{ams.space.s}" }
        },
        "completed": {
          "connector": {
            "background-color": { "value": "{ams.color.feedback.success}" }
          },
          "marker": {
            "background-color": { "value": "{ams.color.feedback.success}" }
          }
        },
        "connector": {
          "background-color": { "value": "{ams.color.separator}" },
          "inline-size": { "value": "{ams.border.width.m}" }
        },
        "current": {
          "connector": {
            "background-color": { "value": "{ams.color.feedback.success}" }
          },
          "indicator": {
            "background-color": { "value": "{ams.color.feedback.success}" }
          },
          "marker": {
            "background-color": { "value": "{ams.color.feedback.success}" }
          }
        },
        "heading-2": {
          "icon": {
            "block-size": { "value": "calc({ams.typography.heading.2.font-size} * 0.8)" }
          },
          "marker": {
            "block-size": {
              "value": "calc({ams.typography.heading.2.font-size} * {ams.typography.heading.2.line-height})"
            },
            "margin-block-start": {
              "value": "calc(({ams.typography.heading.2.line-height} * {ams.typography.heading.2.font-size}) / 2)"
            }
          },
          "shape": {
            "block-size": { "value": "calc({ams.typography.heading.2.font-size} * 0.8)" },
            "inline-size": { "value": "calc({ams.typography.heading.2.font-size} * 0.8)" }
          }
        },
        "heading-3": {
          "icon": {
            "block-size": { "value": "calc({ams.typography.heading.3.font-size} * 0.8)" }
          },
          "marker": {
            "block-size": {
              "value": "calc({ams.typography.heading.3.font-size} * {ams.typography.heading.3.line-height})"
            },
            "margin-block-start": {
              "value": "calc(({ams.typography.heading.3.line-height} * {ams.typography.heading.3.font-size}) / 2)"
            }
          },
          "shape": {
            "block-size": { "value": "calc({ams.typography.heading.3.font-size} * 0.8)" },
            "inline-size": { "value": "calc({ams.typography.heading.3.font-size} * 0.8)" }
          }
        },
        "heading-4": {
          "icon": {
            "block-size": { "value": "calc({ams.typography.heading.4.font-size} * 0.9)" }
          },
          "marker": {
            "block-size": {
              "value": "calc({ams.typography.heading.4.font-size} * {ams.typography.heading.4.line-height})"
            },
            "margin-block-start": {
              "value": "calc(({ams.typography.heading.4.line-height} * {ams.typography.heading.4.font-size}) / 2)"
            }
          },
          "shape": {
            "block-size": { "value": "calc({ams.typography.heading.4.font-size} * 0.9)" },
            "inline-size": { "value": "calc({ams.typography.heading.4.font-size} * 0.9)" }
          }
        },
        "indicator": {
          "padding-inline": {
            "value": "{ams.space.l}"
          }
        },
        "marker": {
          "background-color": { "value": "{ams.color.separator}" }
        },
        "medium": {
          "indicator": {
            "padding-inline": {
              "value": "{ams.space.m}"
            }
          }
        }
      }
    }
  }
}
