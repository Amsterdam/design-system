{
  "ams": {
    "progress-list": {
      "heading-2": {
        "step": {
          "marker": {
            "block-size": {
              "value": "calc({ams.typography.heading.2.font-size} * {ams.typography.heading.2.line-height})"
            },
            "margin-block-start": {
              "value": "calc(({ams.typography.heading.2.line-height} * {ams.typography.heading.2.font-size}) / 2)"
            }
          }
        }
      },
      "heading-3": {
        "step": {
          "marker": {
            "block-size": {
              "value": "calc({ams.typography.heading.3.font-size} * {ams.typography.heading.3.line-height})"
            },
            "margin-block-start": {
              "value": "calc(({ams.typography.heading.3.line-height} * {ams.typography.heading.3.font-size}) / 2)"
            }
          }
        }
      },
      "heading-4": {
        "step": {
          "marker": {
            "block-size": {
              "value": "calc({ams.typography.heading.4.font-size} * {ams.typography.heading.4.line-height})"
            },
            "margin-block-start": {
              "value": "calc(({ams.typography.heading.4.line-height} * {ams.typography.heading.4.font-size}) / 2)"
            }
          }
        }
      },
      "step": {
        "gap": {
          "value": "{ams.space.m}"
        },
        "medium": {
          "gap": {
            "value": "{ams.space.l}"
          }
        },
        "shape": {
          "block-size": {
            "value": "1.5rem"
          },
          "inline-size": {
            "value": "1.5rem"
          },
          "background-color": {
            "value": "{ams.color.background}"
          },
          "border-color": {
            "value": "{ams.color.progress.upcoming}"
          },
          "border-style": {
            "value": "solid"
          },
          "border-width": {
            "value": "{ams.border.width.m}"
          },
          "outline-color": {
            "value": "{ams.color.background}"
          },
          "outline-style": {
            "value": "solid"
          },
          "outline-width": {
            "value": "{ams.border.width.m}"
          }
        },
        "icon": {
          "color": {
            "value": "{ams.color.background}"
          }
        },
        "connector": {
          "border-inline-color": {
            "value": "{ams.color.progress.upcoming}"
          },
          "border-inline-style": {
            "value": "dashed"
          },
          "border-inline-width": {
            "value": "1px"
          }
        },
        "heading": {
          "padding-block-end": {
            "value": "{ams.space.s}"
          }
        },
        "body": {
          "padding-block-end": {
            "value": "{ams.space.l}"
          }
        },
        "sub-steps": {
          "margin-block-start": {
            "value": "{ams.space.s}"
          },
          "step": {
            "indicator": {
              "padding-inline-end": {
                "value": "{ams.space.m}"
              },
              "medium": {
                "padding-inline": {
                  "value": "{ams.space.l}"
                },
                "margin-inline-start": {
                  "value": "calc(-1 * {ams.progress-list.step.medium.gap} - {ams.progress-list.step.shape.inline-size})"
                }
              }
            },
            "marker": {
              "block-size": {
                "value": "calc({ams.typography.body-text.font-size} * {ams.typography.body-text.line-height})"
              }
            },
            "shape": {
              "block-size": {
                "value": "0.75rem"
              },
              "background-color": {
                "value": "{ams.color.background}"
              },
              "border-color": {
                "value": "{ams.color.progress.upcoming}"
              },
              "border-style": {
                "value": "solid"
              },
              "border-width": {
                "value": "{ams.border.width.m}"
              },
              "outline-color": {
                "value": "{ams.color.background}"
              },
              "outline-style": {
                "value": "solid"
              },
              "outline-width": {
                "value": "{ams.border.width.m}"
              }
            },
            "connector": {
              "background-color": {
                "value": "{ams.color.background}"
              },
              "border-inline-color": {
                "value": "{ams.color.progress.upcoming}"
              },
              "border-inline-style": {
                "value": "dashed"
              },
              "border-inline-width": {
                "value": "1px"
              },
              "last-step": {
                "border-inline-color": {
                  "value": "{ams.color.background}"
                }
              },
              "margin-block-start": {
                "value": "calc({ams.typography.body-text.font-size} * {ams.typography.body-text.line-height} / 2)"
              }
            },
            "body": {
              "padding-block-end": {
                "value": "{ams.space.m}"
              }
            },
            "completed": {
              "shape": {
                "background-color": {
                  "value": "{ams.color.progress.completed}"
                },
                "border": {
                  "value": "none"
                }
              },
              "connector": {
                "background-color": {
                  "value": "{ams.color.progress.completed}"
                },
                "border-inline-color": {
                  "value": "{ams.color.progress.completed}"
                },
                "border-inline-style": {
                  "value": "solid"
                }
              }
            },
            "current": {
              "shape": {
                "background-color": {
                  "value": "{ams.color.progress.completed}"
                },
                "border-color": {
                  "value": "{ams.color.progress.current}"
                }
              }
            }
          }
        },
        "completed": {
          "shape": {
            "background-color": {
              "value": "{ams.color.progress.completed}"
            },
            "border": {
              "value": "none"
            }
          },
          "connector": {
            "background-color": {
              "value": "{ams.color.progress.completed}"
            },
            "border-inline-color": {
              "value": "{ams.color.progress.completed}"
            },
