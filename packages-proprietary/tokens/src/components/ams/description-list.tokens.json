{
  "ams": {
    "description-list": {
      "color": {
        "$value": "{ams.color.text.default}",
        "$type": "color"
      },
      "column-gap": {
        "$value": "{ams.space.l}",
        "$extensions": {
          "ams.type": "columnGap"
        }
      },
      "font-family": {
        "$value": "{ams.typography.font-family}",
        "$type": "fontFamily"
      },
      "font-size": {
        "$value": "{ams.typography.body-text.font-size}",
        "$extensions": {
          "ams.type": "fontSize"
        }
      },
      "line-height": {
        "$value": "{ams.typography.body-text.line-height}",
        "$extensions": {
          "ams.type": "lineHeight"
        }
      },
      "row-gap": {
        "$value": "{ams.space.s}",
        "$extensions": {
          "ams.type": "rowGap"
        }
      },
      "inverse": {
        "color": {
          "$value": "{ams.color.text.inverse}",
          "$type": "color"
        }
      },
      "term": {
        "font-weight": {
          "$value": "{ams.typography.body-text.bold.font-weight}",
          "$type": "fontWeight"
        }
      },
      "description": {
        "font-weight": {
          "$value": "{ams.typography.body-text.font-weight}",
          "$type": "fontWeight"
        },
        "padding-inline-start": {
          "$value": "{ams.space.l}",
          "$extensions": {
            "ams.type": "paddingInline"
          }
        }
      },
      "narrow": {
        "grid-template-columns": {
          "$value": "1fr 4fr",
          "$extensions": {
            "ams.type": "gridTemplateColumns"
          }
        }
      },
      "medium": {
        "grid-template-columns": {
          "$value": "1fr 2fr",
          "$extensions": {
            "ams.type": "gridTemplateColumns"
          }
        }
      },
      "wide": {
        "grid-template-columns": {
          "$value": "1fr 1fr",
          "$extensions": {
            "ams.type": "gridTemplateColumns"
          }
        }
      }
    }
  }
}
