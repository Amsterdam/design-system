{
  "ams": {
    "grid": {
      "column-count": {
        "$value": "4",
        "$type": "number"
      },
      "column-gap": {
        "$value": "{ams.space.xl}",
        "$extensions": {
          "ams.type": "columnGap"
        }
      },
      "padding-block": {
        "l": {
          "$value": "{ams.space.l}",
          "$extensions": {
            "ams.type": "paddingBlock"
          }
        },
        "xl": {
          "$value": "{ams.space.xl}",
          "$extensions": {
            "ams.type": "paddingBlock"
          }
        },
        "2xl": {
          "$value": "{ams.space.2xl}",
          "$extensions": {
            "ams.type": "paddingBlock"
          }
        }
      },
      "padding-inline": {
        "$value": "{ams.space.l}",
        "$extensions": {
          "ams.type": "paddingInline"
        }
      },
      "row-gap": {
        "l": {
          "$value": "{ams.space.l}",
          "$extensions": {
            "ams.type": "rowGap"
          }
        },
        "xl": {
          "$value": "{ams.space.xl}",
          "$extensions": {
            "ams.type": "rowGap"
          }
        },
        "2xl": {
          "$value": "{ams.space.2xl}",
          "$extensions": {
            "ams.type": "rowGap"
          }
        }
      },
      "medium": {
        "column-count": {
          "$value": "8",
          "$type": "number"
        },
        "padding-inline": {
          "$value": "{ams.space.xl}",
          "$extensions": {
            "ams.type": "paddingInline"
          }
        }
      },
      "wide": {
        "column-count": {
          "$value": "12",
          "$type": "number"
        },
        "padding-inline": {
          "$value": "{ams.space.2xl}",
          "$extensions": {
            "ams.type": "paddingInline"
          }
        }
      }
    }
  }
}
