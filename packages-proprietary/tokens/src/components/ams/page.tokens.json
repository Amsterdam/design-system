{
  "ams": {
    "page": {
      "background-color": {
        "$value": "{ams.color.background}",
        "$type": "color"
      },
      "max-inline-size": {
        "$value": {
          "value": 90,
          "unit": "rem"
        },
        "$type": "dimension"
      },
      "with-menu": {
        "max-inline-size": {
          "$value": {
            "value": 120,
            "unit": "rem"
          },
          "$type": "dimension"
        },
        "area-skip-link": {
          "margin-inline-end": {
            "$value": "{ams.grid.padding-inline}",
            "comment": "Must be the Grid inline padding, to make sure Skip Link and Grid line up",
            "$extensions": {
              "ams.type": "marginInline"
            }
          }
        }
      }
    }
  }
}
