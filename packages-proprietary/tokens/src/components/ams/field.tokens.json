{
  "ams": {
    "field": {
      "gap": {
        "$value": "{ams.space.s}",
        "$type": "dimension"
      },
      "invalid": {
        "border-inline-start": {
          "$value": "{ams.border.width.l} solid {ams.color.feedback.error}",
          "$type": "color"
        },
        "padding-inline-start": {
          "$value": "{ams.space.m}",
          "$type": "dimension"
        }
      }
    }
  }
}
