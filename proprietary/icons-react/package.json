{
  "version": "0.0.1-alpha.0",
  "author": "Community for NL Design System",
  "description": "React icon components for the City of Amsterdam",
  "license": "SEE LICENSE IN LICENSE.md",
  "name": "@amsterdam/icons-react",
  "keywords": [
    "nl-design-system"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "static"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Amsterdam/design-system.git",
    "directory": "proprietary/icons-react"
  },
  "scripts": {
    "clean": "rimraf src/ && rimraf dist/",
    "svgr": "svgr --jsx-runtime automatic --typescript --svg-props focusable={false} --out-dir src -- ../icons/static",
    "build": "npm run clean && npm run svgr && rollup -c",
    "install": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.0.1",
    "@svgr/cli": "6.5.1",
    "@types/react": "18.0.27",
    "react": "18.2.0",
    "rollup": "3.14.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.34.1",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "react": "16 - 18",
    "react-dom": "16 - 18"
  }
}
